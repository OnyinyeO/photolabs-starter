{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended"],
  "overrides": [
    {
      "files": ["*.{js,jsx,ts,tsx}"],
      "rules": {
        "indent": ["error", 2, { "SwitchCase": 1 }]
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {}
}
